  },
  "homepage": "https://github.com/mathebuddy//mathebuddy-vscode-lang-ext#readme",
  "contributes": {
    "languages": [
      {
        "id": "mbl",
        "aliases": [
          "MatheBuddyLanguage",
          "mbl"
        ],
        "extensions": [
          ".mbl"
        ],
        "configuration": "./language-configuration.json"
