  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathebuddy/mathebuddy-vscode-lang-ext.git"
  },
  "author": "Andreas Schwenk, TH KÃ¶ln",
  "license": "GPL-3.0-or-later",
  "bugs": {
